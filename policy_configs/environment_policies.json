  "policies": {
    "dexterous_pick_place": {
      "display_name": "Dexterous Pick & Place",
      "description": "General pick and place operations with various object types and grasp strategies",
      "target_objects": ["all_manipulable"],
      "isaac_lab_task": "pick_place",
      "randomizers": [
        {"name": "object_scatter", "enabled": true, "frequency": "per_frame", "parameters": {"min_objects": 5, "max_objects": 20}},
        {"name": "material_variation", "enabled": true, "frequency": "per_frame"},
        {"name": "lighting_variation", "enabled": true, "frequency": "per_episode"}
      ],
      "capture_config": {
        "annotations": ["rgb", "depth", "semantic_segmentation", "instance_segmentation", "bounding_box_2d", "bounding_box_3d", "object_pose"],
        "resolution": [1280, 720]
      },
      "reward_components": ["grasp_success", "placement_accuracy", "collision_penalty", "efficiency_bonus"]
    },

    "articulated_access": {
      "display_name": "Articulated Access",
      "description": "Opening doors, drawers, and other articulated mechanisms",
      "target_objects": ["articulated_furniture", "articulated_appliance"],
      "isaac_lab_task": "articulated_manipulation",
      "randomizers": [
        {"name": "articulation_state", "enabled": true, "frequency": "per_episode", "parameters": {"open_probability": 0.5}},
        {"name": "object_placement", "enabled": true, "frequency": "per_frame"},
        {"name": "lighting_variation", "enabled": true, "frequency": "per_episode"}
      ],
      "capture_config": {
        "annotations": ["rgb", "depth", "semantic_segmentation", "joint_states"]
      },
      "reward_components": ["joint_progress", "handle_grasp", "smooth_motion", "collision_penalty"]
    },

    "drawer_manipulation": {
      "display_name": "Drawer Manipulation",
      "description": "Opening and closing drawers, accessing contents",
      "target_objects": ["drawer", "drawer_contents"],
      "isaac_lab_task": "drawer_open_close",
      "randomizers": [
        {"name": "drawer_state", "enabled": true, "frequency": "per_episode", "parameters": {"open_range": [0.0, 1.0]}},
        {"name": "drawer_contents", "enabled": true, "frequency": "per_episode", "parameters": {"fill_ratio": [0.2, 0.8]}},
        {"name": "lighting_variation", "enabled": true, "frequency": "per_episode"}
      ],
      "capture_config": {
        "annotations": ["rgb", "depth", "semantic_segmentation", "joint_states"]
      },
      "reward_components": ["joint_progress", "handle_grasp", "smooth_motion"]
    },

    "door_manipulation": {
      "display_name": "Door Manipulation",
      "description": "Opening and closing doors of various types",
      "target_objects": ["door", "cabinet_door", "appliance_door"],
      "isaac_lab_task": "door_open_close",
      "randomizers": [
        {"name": "door_state", "enabled": true, "frequency": "per_episode", "parameters": {"open_range": [0.0, 1.57]}},
        {"name": "handle_type", "enabled": true, "frequency": "once"},
        {"name": "lighting_variation", "enabled": true, "frequency": "per_episode"}
      ],
      "capture_config": {
        "annotations": ["rgb", "depth", "semantic_segmentation", "joint_states", "handle_keypoints"]
      },
      "reward_components": ["joint_progress", "handle_grasp", "smooth_motion", "collision_penalty"]
    },

    "dish_loading": {
      "display_name": "Dish Loading",
      "description": "Loading dishes into dishwashers from various locations",
      "target_objects": ["dishes", "utensils", "containers"],
      "isaac_lab_task": "dish_loading",
      "randomizers": [
        {"name": "object_scatter", "enabled": true, "frequency": "per_frame", "parameters": {"min_objects": 5, "max_objects": 25, "regions": ["sink_region", "counter_region", "table_region"]}},
        {"name": "dirty_state", "enabled": true, "frequency": "per_frame", "parameters": {"dirty_probability": 0.7}},
        {"name": "dishwasher_state", "enabled": true, "frequency": "per_episode", "parameters": {"loaded_probability": 0.3}}
      ],
      "capture_config": {
        "annotations": ["rgb", "depth", "semantic_segmentation", "instance_segmentation", "bounding_box_2d"]
      },
      "reward_components": ["dish_placed", "rack_utilization", "collision_penalty", "breakage_penalty"]
    },

    "table_clearing": {
      "display_name": "Table Clearing",
      "description": "Clearing dishes and items from table surfaces",
      "target_objects": ["dishes", "utensils", "food_items"],
      "isaac_lab_task": "table_clearing",
      "randomizers": [
